{
  "dry_run": true,
  "stake_currency": "USDT",
  "stake_amount": 100,
  "trading_mode": "spot",
  "timeframe": "1h",
  "max_open_trades": 1,
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],
  "exchange": {
    "name": "kraken",
    "ccxt_config": {
      "enableRateLimit": true
    },
    "pair_whitelist": [
      "ETH/USDT"
    ]
  },
  "order_types": {
    "entry": "limit",
    "exit": "limit",
    "stoploss": "market",
    "emergency_exit": "market"
  },
  "order_time_in_force": {
    "entry": "GTC",
    "exit": "GTC"
  },
  "unfilledtimeout": {
    "entry": 20,
    "exit": 10,
    "unit": "minutes"
  },
  "dataformat_ohlcv": "feather",
  "api_server": {
    "enabled": false,
    "listen_ip_address": "127.0.0.1",
    "listen_port": 8080,
    "enable_openapi": false,
    "CORS_origins": []
  }
}
